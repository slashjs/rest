{
	"compilerOptions": {
		"esModuleInterop": true,
		"alwaysStrict": true,
		"declaration": true,
		"strict": true,
		"lib": ["esnext"],
		"module": "commonjs",
		"moduleResolution": "node",
		"noUnusedLocals": true,
		"outDir": "./dist",
		"removeComments": false,
		"resolveJsonModule": true,
		"target": "es2020",
		"types": []
	},
	"exclude": ["node_modules", "dist"]
}
